# Environment variables template
# Copy this file to .env and fill in your actual values

# Weights & Biases API Key
# Get your key from: https://wandb.ai/authorize
WANDB_API_KEY=your_key_here

# Weights & Biases defaults
WANDB_ENTITY=thordeibert-danmarks-tekniske-universitet-dtu
WANDB_PROJECT=MLOps_project

# Inference artifact fallback (used when WANDB_SWEEP_ID is not set)
WANDB_ARTIFACT=ai_vs_human_model:latest

# Optional: load best model from a sweep
# Set WANDB_SWEEP_ID to enable sweep-based model selection; metric defaults to train/epoch_acc
WANDB_SWEEP_ID=
WANDB_SWEEP_METRIC=train/epoch_acc
